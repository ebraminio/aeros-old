+brainfuck.srcinfo:
+brainfuck.srcman:
+brainfuck.uninstall:
+	$(RM) -f gcc-brainfuck$(exeext)
+	$(RM) -f brainfuck1$(exeext)
+	$(RM) -f $(brainfuck_OBJS)
